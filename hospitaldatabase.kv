
<MyItem>
    text: "Two-line item with avatar"
    secondary_text: "Secondary text here"
    _no_ripple_effect: True

    ImageLeftWidget:
        source: "data/logo/kivy-icon-256.png"


ScrnMngr:
    id: screenMngr
    name: 'screenMngr'
    Screen:
        name: 'profile'
        id: profile
        
        MDBottomNavigation:
           
            MDBottomNavigationItem:
                name: 'views'
                text: 'data'
                icon: 'database'
                      
                MDBoxLayout:
                    orientation: "vertical"
                
                    MDTopAppBar:
                        id: toolbar
                        title: app.title
                        md_bg_color: 1, 1, 1, 1
                
                    MDBoxLayout:
                        adaptive_height: True                        
                        padding: dp(30)   
                        size_hint: 1, 0.3  
                        MDTextField:
                            id: search_field
                            hint_text: 'Search icon'
                            on_text: root.search_folder(self.text)
                            icon_right: 'magnify'
                        
                    ScrollView:                
                        MDSelectionList:
                            id: selection_list
                            spacing: "12dp"
                            overlay_color: app.overlay_color[:-1] + [.2]
                            icon_bg_color: app.overlay_color
                            on_selected: app.on_selected(*args)
                            on_unselected: app.on_unselected(*args)
                            on_selected_mode: app.set_selection_mode(*args)
                
                MDFloatingActionButton:
                    icon: 'plus-thick'
                    pos_hint: {'center_x':0.8,'center_y':0.15}  
                    on_press: screenMngr.current = 'add'
                        
            MDBottomNavigationItem:
                name: 'chat'
                text: 'chat'
                icon: 'chat'
                MDLabel:
                    text:'chat'
